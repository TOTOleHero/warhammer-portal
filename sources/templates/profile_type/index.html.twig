{% extends 'base.html.twig' %}

{% block title %}ProfileType index{% endblock %}

{% block body %}
    <h1>ProfileType index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for profile_type in profile_types %}
            <tr>
                <td>{{ profile_type.id }}</td>
                <td>
                    <a href="{{ path('profile_type_show', {'id': profile_type.id}) }}">show</a>
                    <a href="{{ path('profile_type_edit', {'id': profile_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('profile_type_new') }}">Create new</a>
{% endblock %}
