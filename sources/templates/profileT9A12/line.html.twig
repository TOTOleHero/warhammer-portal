

{% set profile = profileT9A12 %}
{% block body2 %}
{% if startTable is not defined or startTable  %}
<table   class="table">
    <tbody>
        <tr class="table-primary">
            {# <th title="Id" ></th> #}
            <th title="Name">Name</th>
            <th title="GlobalAdvanceRate">Adv</th>
            <th title="GlobalMarch">Mar</th>
            <th title="GlobalDiscipline">Dis</th>
            <th title="DefensiveHealthPoints">HP</th>
            <th title="DefensiveSkill">Def</th>
            <th title="DefensiveResilience">Res</th>
            <th title="DefensiveArmour">Arm</th>
            <th title="OffensiveAttack">Att</th>
            <th title="OffensiveSkill">Off</th>
            <th title="OffensiveStrength">Str</th>
            <th title="OffensiveArmourPenetration">AP</th>
            <th title="OffensiveAgility">Agi</th>
        </tr>
        {% endif %}
        <tr>
            {#<td>{{ profileT9A12.id }}</td> #}
            <td  scope="row" class="table-secondary">{{ profile.name }}</td>


            <td>{{ profileT9A12.globalAdvanceRate }}</td>
            <td>{{ profileT9A12.globalMarch }}</td>
            <td>{{ profileT9A12.globalDiscipline }}</td>
            <td>{{ profileT9A12.defensiveHealthPoints }}</td>
            <td>{{ profileT9A12.defensiveSkill }}</td>
            <td>{{ profileT9A12.defensiveResilience }}</td>
            <td>{{ profileT9A12.defensiveArmour }}</td>
            <td>{{ profileT9A12.offensiveAgility }}</td>
            <td>{{ profileT9A12.offensiveAttack }}</td>
            <td>{{ profileT9A12.offensiveSkill }}</td>
            <td>{{ profileT9A12.offensiveStrength }}</td>
            <td>{{ profileT9A12.offensiveArmourPenetration }}</td>



        </tr>
        <tr>
            {# <td>{{ profileAOS4.id }}</td> #}
            <td ></td>
            <td scope="row">Equipments</td>
            <td colspan="11">{% for equipment in profile.equipments %}{% if not loop.first %}, {% endif %}{{ equipment }}{% else %}No equipment{% endfor %}.</td>
        </tr>
        {% if endTable is not defined or endTable  %}
    </tbody>
</table>
{% endif %}
{% endblock %}