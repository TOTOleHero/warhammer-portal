<form method="post" action="{{ path('unitGeneric_delete', {'id': unitGeneric.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" base_name="_method" value="DELETE">
    <input type="hidden" base_name="_token" value="{{ csrf_token('delete' ~ unitGeneric.id) }}">
    <button class="btn">Delete</button>
</form>
